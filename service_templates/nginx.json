{
	"$schema": "https://raw.githubusercontent.com/kristiankunc/svs-core/refs/heads/main/service_templates/schema.json",
	"args": [],
	"default_env": {},
	"default_ports": [
		{
			"container": 80,
			"host": null
		}
	],
	"default_volumes": [
		{
			"/usr/share/nginx/html": null
		},
		{
			"/etc/nginx/conf.d": null
		}
	],
	"description": "A minimal NGINX web server with default config",
	"healthcheck": {
		"interval": 30,
		"retries": 3,
		"start_period": 5,
		"test": [
			"CMD",
			"curl",
			"-f",
			"http://localhost/"
		],
		"timeout": 10
	},
	"image": "nginx:latest",
	"name": "nginx-webserver",
	"start_cmd": null,
	"type": "image"
}
