{
	"$schema": "https://raw.githubusercontent.com/kristiankunc/svs-core/refs/heads/main/service_templates/schema.json",
	"args": [],
	"default_contents": [
		{
			"content": "<html>\n<head><title>Welcome to NGINX!</title></head>\n<body>\n<h1>Success! NGINX is running.</h1>\n</body>\n</html>\n",
			"location": "/config/www/index.html"
		}
	],
	"default_env": [],
	"default_ports": [
		{
			"container": 80,
			"host": null
		}
	],
	"default_volumes": [
		{
			"container": "/config",
			"host": null
		}
	],
	"description": "A minimal NGINX web server with default config",
	"healthcheck": {
		"interval": 30,
		"retries": 3,
		"start_period": 5,
		"test": [
			"CMD",
			"curl",
			"-f",
			"http://localhost/"
		],
		"timeout": 10
	},
	"image": "lscr.io/linuxserver/nginx:1.28.0",
	"name": "nginx-webserver",
	"start_cmd": null,
	"type": "image"
}
